{
    "name": "webfoersterei/json-param-converter-bundle",
    "description": "ParamConverter for symfony to convert JSON-requests and validate them",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Timo FÃ¶rster",
            "email": "tfoerster@webfoersterei.de"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/framework-bundle": "~6.0",
        "sensio/framework-extra-bundle": "~6.0",
        "symfony/validator": "~6.0",
        "symfony/serializer": "~6.0",
        "symfony/property-access": "~6.0",
        "symfony/property-info": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Webfoersterei\\Bundle\\JsonParamConverterBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webfoersterei\\Bundle\\JsonParamConverterBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
